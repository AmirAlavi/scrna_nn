#!/usr/bin/env python

# import pdb; pdb.set_trace()

import numpy as np

from scrna_nn.util import cli

if __name__ == '__main__':
    print("NOTE: setting random seed for reproducibility")
    np.random.seed(34792)
    parser = cli.create_parser()
    args = parser.parse_args()
    print(args)
    args.func(args)
